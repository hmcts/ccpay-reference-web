{% block top_of_page %}{% endblock %}
<!DOCTYPE html>
<!--[if lt IE 9]><html class="lte-ie8" lang="{{ html_lang|default('en') }}"><![endif]-->
<!--[if gt IE 8]><!--><html lang="{{ html_lang|default('en') }}"><!--<![endif]-->
<head>
  <meta charset="utf-8" />
  <title>{{ title }}</title>

  <!--[if gt IE 8]><!--><link href="{{ asset_paths['style_vendor'] }}/govuk-template.css?0.18.3" media="screen" rel="stylesheet" /><!--<![endif]-->
  <!--[if IE 6]><link href="{{ asset_paths['style_vendor'] }}/govuk-template-ie6.css?0.18.3" media="screen" rel="stylesheet" /><![endif]-->
  <!--[if IE 7]><link href="{{ asset_paths['style_vendor'] }}/govuk-template-ie7.css?0.18.3" media="screen" rel="stylesheet" /><![endif]-->
  <!--[if IE 8]><link href="{{ asset_paths['style_vendor'] }}/govuk-template-ie8.css?0.18.3" media="screen" rel="stylesheet" /><![endif]-->
  <link href="{{ asset_paths['style_vendor'] }}/govuk-template-print.css?0.18.3" media="print" rel="stylesheet" />

  <!--[if IE 8]><link href="{{ asset_paths['style_vendor'] }}/fonts-ie8.css?0.18.3" media="all" rel="stylesheet" /><![endif]-->
  <!--[if gte IE 9]><!--><link href="{{ asset_paths['style_vendor'] }}/fonts.css?0.18.3" media="all" rel="stylesheet" /><!--<![endif]-->

  <link rel="mask-icon" href="{{ asset_paths['style_vendor'] }}/gov.uk_logotype_crown.svg?0.18.3" color="#0b0c0c">
  <link rel="apple-touch-icon-precomposed" sizes="152x152" href="{{ asset_paths['images_vendor'] }}images/apple-touch-icon-152x152.png?0.18.3">
  <link rel="apple-touch-icon-precomposed" sizes="120x120" href="{{ asset_paths['images_vendor'] }}images/apple-touch-icon-120x120.png?0.18.3">
  <link rel="apple-touch-icon-precomposed" sizes="76x76" href="{{ asset_paths['images_vendor'] }}images/apple-touch-icon-76x76.png?0.18.3">
  <link rel="apple-touch-icon-precomposed" href="{{ asset_paths['images_vendor'] }}images/apple-touch-icon-60x60.png?0.18.3">

  {% if development %}
    <link href="{{ asset_paths['style_vendor'] }}/HTMLCSAuditor.css" rel="stylesheet" />
  {% endif %}

  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta property="og:image" content="{{ asset_paths['images_vendor'] }}images/opengraph-image.png?0.18.3">

  {% block head %}{% endblock %}
</head>

<body class="{% block body_classes %}{% endblock %}">
<script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

{% block body_start %}{% endblock %}

<div id="skiplink-container">
  <div>
    <a href="#content" class="skiplink">{{ skip_link_message|default('Skip to main content') }}</a>
  </div>
</div>

<div id="global-cookie-message">

  {% block cookie_message %}{% endblock %}

</div>


<header role="banner" id="global-header" class="{% block header_class %}{% endblock %}">
  <div class="header-wrapper">
    <div class="header-global">
      <div class="header-logo">
        <a href="{{ homepage_url|default('https://www.gov.uk') }}" title="{{ logo_link_title|default('Go to the GOV.UK homepage') }}" id="logo" class="content">
          <img src="{{ asset_paths['images_vendor'] }}/gov.uk_logotype_crown_invert_trans.png?0.18.3" width="36" height="32" alt=""> {{ global_header_text|default('GOV.UK') }}
        </a>
      </div>
      {% block inside_header %}{% endblock %}
    </div>
    {% block proposition_header %}{% endblock %}
  </div>
</header>


{% block after_header %}{% endblock %}

<div id="global-header-bar"></div>
<main id="content" role="main">
  {% include "includes/phase_banner_alpha.njk" %}
  <h1 class="heading-xlarge">
    {{ heading }}
  </h1>
  {% block content %}{% endblock %}
</main>

<footer class="group js-footer" id="footer" role="contentinfo">

  <div class="footer-wrapper">
    {% block footer_top %}{% endblock %}

    <div class="footer-meta">
      <div class="footer-meta-inner">
        {% block footer_support_links %}{% endblock %}

        <div class="open-government-licence">
          <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence</a></p>

          {% block licence_message %}<p>{{ 'All content is available under the <a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence v3.0</a>, except where otherwise stated' | safe }}</p>{% endblock %}

        </div>
      </div>

      <div class="copyright">
        <a href="http://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/">{{ crown_copyright_message|default('&copy; ' + 'Crown copyright')|safe }}</a>
      </div>
    </div>
  </div>
</footer>

<div id="global-app-error" class="app-error hidden"></div>

<script src="{{ asset_paths['js_vendor'] }}/govuk-template.js?0.18.3"></script>

{% block body_end %}{% endblock %}


<script>if (typeof window.GOVUK === 'undefined') document.body.className = document.body.className.replace('js-enabled', '');</script>
</body>
</html>
